{
  "root": true,
  "extends": ["@axodotdev/eslint-config", "plugin:vitest-globals/recommended"],
  "env": {
    "vitest-globals/env": true
  },
  "globals": {
    "defineEventHandler": true,
    "$fetch": true,
    "useState": true,
    "useRuntimeConfig": true,
    "useRouter": true,
    "useFetch": true,
    "useRoute": true,
    "useRequestHeaders": true,
    "useCookie": true,
    "useHead": true,
    "navigateTo": true,
    "defineNuxtConfig": true,
    "definePageMeta": true,
    "defineNuxtPlugin": true
  }
}
